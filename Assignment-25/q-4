#include <iostream>
using namespace std;
class LargestNumber{
    int a,b,c;
    public:
    void set(){
        cin>>a>>b>>c;
    }
    void print(){
        if(a>b&&a>c)cout<<a<<" is greatest";
        else if(b>a&&b>c)cout<<b<<" is greatest";
        else cout<<c<<" is greatest";
    }
};
int main()
{
    LargestNumber n;
    n.set();
    n.print();
    return 0;
}
