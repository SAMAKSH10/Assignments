#include <iostream>
using namespace std;
class Factorial{
    int n;
    public:
    void set(){
        cin>>n;
    }
    void print(){
        int i,y,fact=1;
        if(n==1||n==0) cout<<"Factorial of the number is 1";
        for(;n>1;n--){
            fact=fact*n;
        }
        cout<<"Factorial of the number is "<<fact;
    }
};
int main()
{
    Factorial f;
    f.set();
    f.print();
    return 0;
}
